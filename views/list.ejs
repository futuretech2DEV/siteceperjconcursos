<%- include('top.ejs') %>
<div class="row" style="background-color: black; color: white; height: 30px; border-radius: 5px 5px 0px 0px;">
  <div class="col-2">Protocolo</div>
  <div class="col">Nome</div>
  <div class="col-2">Ações</div>
</div>
<% docs.forEach(function(customer){ %>
  <div class="row" style="border: 1px solid rgba(39,41,43,0.1); background-color: rgba(39,41,43,0.03);">
    <div class="col-2">
      <a href="/edit/<%= customer._id %>">
        <%= customer.pProtocolo %>
      </a>
    </div>
    <div class="col">
      <%= customer.pNome %>
    </div>
    <div class="col-2">
      <a href="/editar/<%= customer._id %>" class="btn">
        <img
          src="data:image/png;base64,"
          alt="Editar registro">
      </a>
      <a href="/exportPDF/<%= customer._id %>" class="btn">
        <img
          src="data:image/png;base64,"
          alt="Exportar PDF" srcset="">
      </a>
      <a href="/delete/<%= customer._id %>" class="btn"
        onclick="return confirm('Tem certeza que deseja excluir?');">
        <img
          src="data:image/png;base64,"
          alt="Excluir Registro">
      </a>

    </div>
  </div>
  <% }) %>
  <hr />
  <p>
    <%= count %> clientes encontrados!
  </p>

  <nav aria-label="Paginação">
    <ul class="pagination">
      <% for(let i=1; i <=qtdPaginas; i++) { if(i !==pagina){ %>
        <li class="page-item"><a class="page-link" href="/<%= i %>">
            <%= i %>
          </a></li>
        <% } else { %>
          <li class="page-item active"><a class="page-link" href="/<%= i %>">
              <%= i %>
            </a></li>
          <% } } %>
    </ul>
  </nav>

  <hr />
  <a href="/new" class="btn btn-primary">Cadastrar novo cliente</a>


    

      <%- include('bottom.ejs') %>